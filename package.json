{
	"name": "fruster-template-service",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "fruster-runner ./app.ts",
		"dev": "nodemon --exec \"fruster-runner ./app.ts\"",
		"test": "nyc --reporter=html --check-coverage --lines 90 fruster-runner ./spec/support/jasmine-runner.ts",
		"tunnel:test-1": "fruster-switch test-1 && fruster-run-remote -p nats-pod -a fruster-template-service -e app.ts -r \"nodemon --exec \"fruster-runner ./app.ts\"\"",
		"tunnel:prod": "fruster-switch c2.frost.se && fruster-run-remote -p nats-pod -a fruster-template-service -e app.ts -r \"nodemon --exec \"fruster-runner ./app.ts\"\""
	},
	"dependencies": {
		"@fruster/bus": "1.0.0-alpha.15",
		"@fruster/health": "1.0.0-alpha.15",
		"@fruster/log": "1.0.0-alpha.15",
		"mongodb": "2.2.36"
	},
	"devDependencies": {
		"@fruster/decorators": "1.0.0-alpha.15",
		"@fruster/runner": "1.0.0-alpha.16",
		"@fruster/test-utils": "1.0.0-alpha.15",
		"@types/jasmine": "^5.1.4",
		"@types/mongodb": "2.2.28",
		"@types/node": "^20.10.4",
		"@types/uuid": "^9.0.7",
		"jasmine": "^5.1.0",
		"jasmine-spec-reporter": "^7.0.0",
		"nodemon": "^3.0.2",
		"nyc": "^15.1.0",
		"typescript": "4.9.5"
	},
	"nyc": {
		"exclude": [
			"spec/*",
			"lib/errors.ts",
			"lib/constants.ts",
			"config.ts"
		]
	}
}

{
	"name": "fruster-template-service",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "ts-node ./app.ts",
		"start:watch": "ts-node-dev ./app.ts",
		"test": "nyc --reporter=html --check-coverage --lines 90 ts-node ./spec/support/jasmine-runner.ts",
		"test:watch": "ts-node-dev --respawn --no-notify ./spec/support/jasmine-runner.ts"
	},
	"dependencies": {
		"fruster-bus": "^0.7.6",
		"fruster-errors": "0.1.2",
		"fruster-health": "^0.0.14",
		"fruster-log": "^0.0.18",
		"mongodb": "^2.2.36"
	},
	"devDependencies": {
		"@types/jasmine": "^3.5.11",
		"@types/node": "^14.0.18",
		"@types/uuid": "^8.0.0",
		"@types/mongodb": "2.2.19",
		"fruster-test-utils": "^0.5.0",
		"jasmine": "^3.5.0",
		"jasmine-spec-reporter": "^5.0.2",
		"nyc": "^15.1.0",
		"ts-node": "^8.10.2",
		"ts-node-dev": "^1.0.0-pre.50",
		"typescript": "^3.9.6",
		"fruster-decorators": "0.1.0"
	},
	"nyc": {
		"exclude": [
			"spec/*",
			"lib/errors.ts",
			"lib/constants.ts",
			"lib/docs.ts",
			"config.ts"
		]
	}
}
